      <title>Add Subject To Student</title>
    </head>
    <!-- <link rel="stylesheet" href="/style.css"> -->
    <!-- <link rel="stylesheet" href="/sesuatu.css"> -->
    <body>
      <center>
        <br>
      <form action="/students/<%=dataStud.id%>/addsubject" method="post">

    Firstname :<br>
    <input type="text" name="firstname" value="<%=dataStud.firstname%>">
    <br>

    Lastname :<br>
    <input type="text" name="lastname" value="<%=dataStud.lastname%>">
    <br>

    Email :<br>
    <input type="text" name="email" value="<%=dataStud.email%>">
    <br>

    Choose Subject:<br>
        <select name="dropdown">
            <%dataSub.forEach(d => {%>
            <option value="<%= d.id%>"><%=d.subject_name%></option>
            <%})%>
        </select>
    <br>
    <br>
    <input type="submit" value="Add Subject">

  </form>
